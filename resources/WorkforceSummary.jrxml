	<queryString>
		<![CDATA[SELECT DISTINCT
       co.ID CODCIA
      ,co.NAME NOMCIA
      ,SCE.ID CODSENARIO
      ,SCE.NAME NAME_ESC
      ,SCE.CURRENCY_ID
FROM  PLANC_COMPANY co
     ,SLE_SCENARIO sce
WHERE co.ID=SCE.COMPANY_ID
AND  (SCE.ID = $P{scenarioID})
AND  (co.ID = $P{companyID})
ORDER BY
      co.ID,SCE.ID]]>
